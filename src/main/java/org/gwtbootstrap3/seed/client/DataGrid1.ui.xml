<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"

	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:bgwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:base="urn:import:org.gwtbootstrap3.client.ui.base">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:Container fluid="true">
			<b:Row>
				<b:Column size="MD_12">
					<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE">
						<bgwt:DataGrid bordered="true" ui:field="dataGrid"
							condensed="true" striped="true" hover="true" />
					</g:HorizontalPanel>
				</b:Column>
			</b:Row>
		</b:Container>
	</g:HTMLPanel>
</ui:UiBinder> 